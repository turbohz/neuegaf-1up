{
  "name": "neuegaf-1up",
  "version": "0.0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
 "content_scripts": [
    {
      "matches": ["http://m.neogaf.com/showthread.php*"],
      "js": ["main.js", "thread.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["main.js", "background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "permissions": [
    "declarativeWebRequest",
    "https://*/*"
  ]
}

